{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Основной обзор логов auth-service с фильтрацией",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "builder",
          "expr": "{container=\"slcw-auth-service\"} |~ `$filter` | json | level=\"$level\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Auth Service Logs Explorer",
      "type": "logs"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Статистика по уровням логов",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "ERROR"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "WARN"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "INFO"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "DEBUG"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 2,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "sum by (level) (rate({container=\"slcw-auth-service\"} | json | __error__=\"\" [$__interval]))",
          "legendFormat": "{{level}}",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Log Levels Distribution",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Топ ошибок в auth-service",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 3,
      "options": {
        "showHeader": true
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "topk(10, sum by (msg) (count_over_time({container=\"slcw-auth-service\"} | json | level=\"ERROR\" | __error__=\"\" [$__range])))",
          "queryType": "instant",
          "refId": "A"
        }
      ],
      "title": "Top 10 Errors",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "Value": "Count",
              "msg": "Error Message"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "JWT операции - генерация и валидация токенов",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 15
      },
      "id": 4,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "{container=\"slcw-auth-service\"} | json | msg =~ \".*JWT.*\" | line_format \"{{.time}} [{{.level}}] {{.msg}} {{if .user_id}}user_id={{.user_id}}{{end}} {{if .telegram_id}}telegram_id={{.telegram_id}}{{end}} {{if .error}}error={{.error}}{{end}}\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "JWT Operations",
      "type": "logs"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Операции аутентификации Telegram",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 15
      },
      "id": 5,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "{container=\"slcw-auth-service\"} | json | msg =~ \".*Telegram.*|.*auth.*\" | line_format \"{{.time}} [{{.level}}] {{.msg}} {{if .user_id}}user_id={{.user_id}}{{end}} {{if .username}}username={{.username}}{{end}} {{if .error}}error={{.error}}{{end}}\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Telegram Authentication",
      "type": "logs"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "Операции с базой данных",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 23
      },
      "id": 6,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "{container=\"slcw-auth-service\"} | json | msg =~ \".*PostgreSQL.*|.*database.*|.*User.*\" | line_format \"{{.time}} [{{.level}}] {{.msg}} {{if .user_id}}user_id={{.user_id}}{{end}} {{if .telegram_id}}telegram_id={{.telegram_id}}{{end}} {{if .error}}error={{.error}}{{end}}\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Database Operations",
      "type": "logs"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "${datasource}"
      },
      "description": "HTTP запросы к auth endpoints",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 23
      },
      "id": 7,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "{container=\"slcw-auth-service\"} | json | msg =~ \".*request.*|.*response.*|.*endpoint.*\" | line_format \"{{.time}} [{{.level}}] {{.msg}} {{if .method}}method={{.method}}{{end}} {{if .path}}path={{.path}}{{end}} {{if .status}}status={{.status}}{{end}}\"",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "HTTP Requests",
      "type": "logs"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
